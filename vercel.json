{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/admin/login",
      "destination": "/api/admin/login.js"
    },
    {
      "source": "/api/admin/(.*)",
      "destination": "/api/admin/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/public/admin/$1"
    }
  ]
}