{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/admin/login",
      "destination": "/api/admin/login.js"
    },
    {
      "source": "/api/admin/:path*",
      "destination": "/api/admin/index.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin",
      "destination": "/public/admin/index.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/public/admin/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/public/admin/index.html"
    }
  ]
}
